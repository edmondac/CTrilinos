
include @abs_top_builddir@/../teuchos/Makefile.export.teuchos
include @abs_top_builddir@/../epetra/Makefile.export.epetra

_CTRILINOS_INCLUDES = \
  -I@abs_top_srcdir@/src \
  -I@abs_top_builddir@/src \
  $(EPETRA_INCLUDES) \
  $(TEUCHOS_INCLUDES)

_CTRILINOS_LIBS = @LDFLAGS@ -L@abs_top_builddir@/src -lctrilinos $(EPETRA_LIBS) $(TEUCHOS_LIBS) @LIBS@

@USING_GNUMAKE_TRUE@CTRILINOS_INCLUDES  = $(shell @PERL_EXE@ @abs_top_srcdir@/config/strip_dup_incl_paths.pl $(_CTRILINOS_INCLUDES))
@USING_GNUMAKE_TRUE@CTRILINOS_LIBS      = $(shell @PERL_EXE@ @abs_top_srcdir@/config/strip_dup_libs.pl $(_CTRILINOS_LIBS))
@USING_GNUMAKE_FALSE@CTRILINOS_INCLUDES = $(_CTRILINOS_INCLUDES)
@USING_GNUMAKE_FALSE@CTRILINOS_LIBS     = $(_CTRILINOS_LIBS)
